{
  "name": "react-native-brownfield-macos",
  "version": "0.1.0",
  "description": "Brownfield helpers for React Native",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "./lib/typescript/commonjs/src/index.d.ts",
  "react-native": "src/index",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "ios",
    "macos",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!macos/build",
    "!**/.*"
  ],
  "scripts": {
    "build": "bob build",
    "prepare": "yarn build",
    "nitrogen": "nitro-codegen"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victorhenrion/speedlib.git"
  },
  "author": "Victor <bonjour@victor.id> (https://github.com/victorhenrion)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/victorhenrion/speedlib/issues"
  },
  "homepage": "https://github.com/victorhenrion/speedlib#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@react-native/babel-preset": "0.78.3",
    "@types/react": "~19.0.0",
    "react": "19.0.0",
    "react-native": "0.78.3",
    "react-native-builder-bob": "^0.37.0",
    "typescript": "~5.8.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "codegenConfig": {
    "name": "ReactNativeBrownfield",
    "type": "modules",
    "jsSrcsDir": "./src"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.json",
          "esm": true
        }
      ]
    ]
  }
}
